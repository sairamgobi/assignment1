import java.util.*;
public class Ascii {

	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		String str = s.next().toUpperCase();
		for(int i = 65 ; i <= 90 ; i++ ) {
		 if(str.contains(Character.toString(i))) {
			 System.out.println(Character.toString(i)+" : "+(str.indexOf(i)));
		 }
		 else {
			 System.out.println(Character.toString(i)+" : -1");
		 }
		}
	}

}
